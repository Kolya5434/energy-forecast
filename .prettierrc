{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "endOfLine": "lf",
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^(@mui)/(.*)$",
    "",
    "^@/.*$",
    "^[./]"
  ],
  "importOrderMergeDuplicateImports": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
